extends layout

block content

  header
    nav#mainNav.navbar.navbar-expand-lg.navbar-dark.bg-dark.fixed-top
      a(href="/").navbar-brand.js-scroll-trigger
        img(src="/images/logo.png", width="30" height="30").d-inline-block.align-top
        |  Call of Dutty

      button(type="button", data-toggle="collapse", data-target="#navbarNavAltMarkup", aria-controls="navbarNavAltMarkup", aria-expanded="false", aria-label="Toggle navigation").navbar-toggler
        span.navbar-toggler-icon

      #navbarNavAltMarkup.collapse.navbar-collapse
        ul.navbar-nav.mr-auto
          li.nav-item
            a(href="#section-header").nav-link.js-scroll-trigger
              i.fa.fa-home  Inicio
              span.sr-only (current)

          li.nav-item
            a(href="#principal").nav-link.js-scroll-trigger
              i.fa.fa-adn  Principal

          li.nav-item
            a(href="#biografia").nav-link.js-scroll-trigger
              i.fa.fa-book  Biografia

          li.nav-item
            a(href="#juegos").nav-link.js-scroll-trigger
              i.fa.fa-play  Juegos

          if username
            li.nav-item
              a(href="#").nav-item.nav-link
                i.fa.fa-database  Administrar


        if !username
          #form
            form.well(id='form-login', method='post').form-inline.my-2.my-lg-0
              .input-group
                .input-group-prepend
                  span.input-group-text @
                input#email(type="text", name='email', placeholder="Digite corre electr칩nico", aria-label="email", aria-describedby="email").form-control.mr-sm-2

              .input-group
                .input-group-prepend
                  span.input-group-text #
                input#password(type="password", name='password', placeholder="Digite contrase침a", aria-label="password", aria-describedby="password").form-control.mr-sm-2

              button(type="submit").btn.btn-outline-success.my-2.my-sm-0.mr-sm-2 Iniciar sesi칩n
              button(type="button").btn.btn-outline-primary.my-2.my-sm-0 Registrarse
        else
          ul.navbar-nav.navbar-right
            li.nav-item.dropdown.active
              a#navbarDropdownMenuLink(href="#", data-toggle="dropdown", aria-haspopup="true", aria-expanded="false").nav-link.dropdown-toggle #{username+" "}
                i.fa.fa-user

              #navbarDropdownMenuLink(aria-labelledby="navbarDropdownMenuLink").dropdown-menu
                a(href='/cerrarsession').dropdown-item Cerrar sesi칩n &nbsp;
                  i.fa.fa-close

  section#section-header
    .caption
      span.border FUll Page
      br
      br
      span.border background Image

